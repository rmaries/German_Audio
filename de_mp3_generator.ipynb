   "source": [
    "import gtts\n",
    "counter = 1\n",
    "with open(\"verb_mit_preposition.txt\", 'r') as f:\n",
    "    lines = f.readlines()\n",
    "    for line in lines:\n",
    "        tts = gtts.gTTS(text=line.strip(), lang=\"de\")\n",
    "        tts.save(\"./verb_mit_preposition_voice/vmp_\"+str(counter)+\".mp3\")\n",
    "        counter += 1"
   ]
  }
